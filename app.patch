+            },
+          ]}
+        />
+      </div>
+
+      <div className="flex gap-2 flex-wrap sm:flex-nowrap text-sm">
+        <button
+          type="button"
+          onClick={onStartScan}
+          className="flex items-center gap-1.5 bg-primary hover:bg-primary/90 transition-colors rounded-full text-white font-semibold py-2 px-4"
